<%= form_for [notebook.account, notebook] do |f| %>
  <div class="field">
    <%= f.label :name, 'name' %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :description, 'description' %><br />
    <%= f.text_area :description %>
  </div>
  <div class="field">
    <%= f.submit %>
  </div>
<% end %>
